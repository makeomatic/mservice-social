{
  "name": "mservice-social",
  "description": "mservice-social",
  "main": "./lib/index.js",
  "version": "6.0.4",
  "scripts": {
    "compile": "babel -d ./lib --copy-files ./src",
    "prepublishOnly": "yarn lint && yarn compile",
    "test:e2e": "mdep test run",
    "test": "yarn lint && yarn test:e2e",
    "doc": "cd schemas && apidoc -i ../src/actions -v -o ../docs",
    "docker-release": "mdep docker release",
    "lint": "eslint . --ext .json --ext .js",
    "semantic-release": "semantic-release",
    "release": "yarn --freze-lockfile && yarn semantic-release",
    "start": "mfleet"
  },
  "dependencies": {
    "@hapi/boom": "^8.0.1",
    "@hapi/hapi": "^18.1.0",
    "@microfleet/core": "^15.6.0",
    "@microfleet/plugin-knex": "^0.2.5",
    "@microfleet/transport-amqp": "^15.1.4",
    "@microfleet/validation": "^8.1.2",
    "@sentry/node": "^5.10.1",
    "bluebird": "^3.7.2",
    "bn.js": "^5.0.0",
    "common-errors": "^1.0.5",
    "debug": "^4.0.1",
    "get-value": "^3.0.1",
    "glob": "^7.1.6",
    "is": "^3.2.1",
    "lodash": "^4.17.15",
    "moment": "^2.22.2",
    "ms-conf": "^5.0.2",
    "pg": "^7.14.0",
    "pino": "^5.14.0",
    "prom-client": "^11.5.3",
    "request": "^2.88.0",
    "request-promise": "^4.2.5",
    "retry": "^0.12.0",
    "server-destroy": "^1.0.1",
    "stdout-stream": "^1.4.1",
    "twitter": "^1.7.0",
    "yargs": "^15.0.2"
  },
  "devDependencies": {
    "@babel/cli": "^7.7.5",
    "@babel/core": "^7.7.5",
    "@babel/plugin-proposal-class-properties": "^7.7.4",
    "@babel/plugin-transform-strict-mode": "^7.7.4",
    "@babel/register": "^7.7.4",
    "@makeomatic/deploy": "^9.6.1",
    "@semantic-release/changelog": "^3.0.6",
    "@semantic-release/exec": "^3.3.8",
    "@semantic-release/git": "^7.0.18",
    "apidoc": "^0.19.0",
    "apidoc-plugin-schema": "^0.1.8",
    "babel-eslint": "^10.0.3",
    "babel-plugin-istanbul": "^5.2.0",
    "chance": "^1.1.4",
    "codecov": "^3.6.1",
    "cross-env": "^6.0.3",
    "dotenv": "^8.2.0",
    "eslint": "^6.7.2",
    "eslint-config-makeomatic": "^4.0.0",
    "eslint-plugin-import": "^2.18.2",
    "eslint-plugin-json": "^2.0.1",
    "eslint-plugin-mocha": "^6.2.2",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-unicorn": "^14.0.1",
    "json": "^9.0.6",
    "mocha": "^6.2.2",
    "nyc": "^14.1.1",
    "rimraf": "^3.0.0",
    "sinon": "^7.5.0"
  },
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/makeomatic/mservice-social.git"
  },
  "engines": {
    "node": ">= 10.15.0",
    "npm": ">= 4.0.0"
  },
  "nyc": {
    "include": [
      "src/**/*.js"
    ],
    "exclude": [
      "src/**/*.spec.js"
    ],
    "require": [
      "@babel/register"
    ],
    "sourceMap": false,
    "instrument": false,
    "cache": true,
    "reporter": [
      "lcov",
      "json",
      "text-summary"
    ]
  },
  "husky": {
    "hooks": {
      "commit-msg": "commitlint -e $HUSKY_GIT_PARAMS",
      "prepare-commit-msg": "./node_modules/@makeomatic/deploy/git-hooks/prepare-commit-msg $HUSKY_GIT_PARAMS"
    }
  }
}
