{
  "name": "mservice-social",
  "description": "mservice-social",
  "main": "./lib/index.js",
  "version": "6.4.0",
  "scripts": {
    "compile": "babel -d ./lib --copy-files ./src",
    "prepublishOnly": "yarn lint && yarn compile",
    "test:e2e": "mdep test run",
    "test": "yarn lint && yarn test:e2e",
    "doc": "cd schemas && apidoc -i ../src/actions -v -o ../docs",
    "docker-release": "mdep docker release",
    "lint": "eslint . --ext .json --ext .js",
    "semantic-release": "semantic-release",
    "release": "yarn --freze-lockfile && yarn semantic-release",
    "start": "mfleet"
  },
  "dependencies": {
    "@hapi/boom": "^9.1.0",
    "@hapi/hapi": "^19.1.1",
    "@microfleet/core": "^17.0.0",
    "@microfleet/plugin-knex": "^2.0.0",
    "@microfleet/transport-amqp": "^15.2.0",
    "@microfleet/validation": "^9.0.1",
    "@sentry/node": "^5.15.4",
    "bluebird": "^3.7.2",
    "bn.js": "^5.1.1",
    "common-errors": "^1.0.5",
    "debug": "^4.0.1",
    "get-value": "^3.0.1",
    "glob": "^7.1.6",
    "is": "^3.2.1",
    "lodash": "^4.17.15",
    "moment": "^2.22.2",
    "ms-conf": "^6.0.1",
    "pg": "^7.18.2",
    "pino": "^6.0.0",
    "prom-client": "^12.0.0",
    "request": "^2.88.2",
    "request-promise": "^4.2.5",
    "retry": "^0.12.0",
    "server-destroy": "^1.0.1",
    "stdout-stream": "^1.4.1",
    "twitter": "^1.7.0",
    "yargs": "^15.3.1"
  },
  "devDependencies": {
    "@babel/cli": "^7.8.4",
    "@babel/core": "^7.9.0",
    "@babel/plugin-proposal-class-properties": "^7.8.3",
    "@babel/plugin-transform-strict-mode": "^7.8.3",
    "@babel/register": "^7.9.0",
    "@makeomatic/deploy": "^10.1.4",
    "@semantic-release/changelog": "^5.0.1",
    "@semantic-release/exec": "^5.0.0",
    "@semantic-release/git": "^9.0.0",
    "apidoc": "^0.20.1",
    "apidoc-plugin-schema": "^0.1.8",
    "babel-eslint": "^10.1.0",
    "babel-plugin-istanbul": "^6.0.0",
    "chance": "^1.1.4",
    "codecov": "^3.6.5",
    "cross-env": "^7.0.2",
    "dotenv": "^8.2.0",
    "eslint": "^6.8.0",
    "eslint-config-makeomatic": "^4.0.0",
    "eslint-plugin-import": "^2.20.1",
    "eslint-plugin-json": "^2.1.1",
    "eslint-plugin-mocha": "^6.3.0",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-unicorn": "^18.0.1",
    "json": "^9.0.6",
    "mocha": "^7.1.1",
    "nyc": "^15.0.0",
    "rimraf": "^3.0.2",
    "sinon": "^9.0.1"
  },
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/makeomatic/mservice-social.git"
  },
  "engines": {
    "node": ">= 10.15.0",
    "npm": ">= 4.0.0"
  },
  "nyc": {
    "include": [
      "src/**/*.js"
    ],
    "exclude": [
      "src/**/*.spec.js"
    ],
    "require": [
      "@babel/register"
    ],
    "sourceMap": false,
    "instrument": false,
    "cache": true,
    "reporter": [
      "lcov",
      "json",
      "text-summary"
    ]
  },
  "husky": {
    "hooks": {
      "commit-msg": "commitlint -e $HUSKY_GIT_PARAMS",
      "prepare-commit-msg": "./node_modules/@makeomatic/deploy/git-hooks/prepare-commit-msg $HUSKY_GIT_PARAMS"
    }
  }
}
