{
  "name": "mservice-social",
  "description": "mservice-social",
  "main": "./lib/index.js",
  "version": "0.0.0-development",
  "scripts": {
    "compile": "babel -d ./lib --copy-files ./src",
    "prepublishOnly": "yarn lint && yarn compile",
    "test:e2e": "mdep test run",
    "test": "yarn lint && yarn test:e2e",
    "doc": "cd schemas && apidoc -i ./../src/actions -v -o ./../docs",
    "docker-release": "mdep docker release",
    "lint": "eslint . --ext .json --ext .js",
    "semantic-release": "semantic-release",
    "release": "yarn --freze-lockfile && yarn semantic-release && yarn docker-release && yarn doc",
    "start": "mfleet",
    "commitmsg": "commitlint -e $GIT_PARAMS",
    "preparecommitmsg": "./node_modules/@makeomatic/deploy/git-hooks/prepare-commit-msg $GIT_PARAMS"
  },
  "dependencies": {
    "@microfleet/core": "^10.11.0",
    "@microfleet/transport-amqp": "^13.1.2",
    "bluebird": "^3.5.2",
    "bn.js": "^4.11.6",
    "boom": "^7.2.0",
    "bunyan": "^1.8.8",
    "bunyan-sentry-stream": "^1.1.0",
    "common-errors": "^1.0.5",
    "debug": "^4.0.1",
    "glob": "^7.1.3",
    "hapi": "17.6.0",
    "is": "^3.2.1",
    "knex": "^0.15.2",
    "lodash": "^4.17.11",
    "moment": "^2.22.2",
    "ms-conf": "^3.3.0",
    "ms-validation": "^7.2.0",
    "pg": "^7.5.0",
    "raven": "^2.6.4",
    "request": "^2.88.0",
    "request-promise": "^4.2.2",
    "retry": "^0.12.0",
    "server-destroy": "^1.0.1",
    "stdout-stream": "^1.4.1",
    "twitter": "^1.7.0",
    "yargs": "^12.0.2"
  },
  "devDependencies": {
    "@babel/cli": "^7.0.0",
    "@babel/core": "^7.0.0",
    "@babel/plugin-proposal-class-properties": "^7.0.0",
    "@babel/plugin-proposal-object-rest-spread": "^7.0.0",
    "@babel/plugin-transform-strict-mode": "^7.0.0",
    "@babel/register": "^7.0.0",
    "@makeomatic/deploy": "^7.0.1",
    "apidoc": "^0.17.5",
    "apidoc-plugin-schema": "^0.1.8",
    "babel-eslint": "^10.0.1",
    "babel-plugin-istanbul": "^5.0.1",
    "chance": "^1.0.16",
    "codecov": "^3.1.0",
    "cross-env": "^5.2.0",
    "dotenv": "^6.0.0",
    "eslint": "^5.6.1",
    "eslint-config-makeomatic": "^3.0.0",
    "eslint-plugin-import": "^2.14.0",
    "eslint-plugin-json": "^1.2.1",
    "eslint-plugin-mocha": "^5.2.0",
    "eslint-plugin-promise": "^4.0.1",
    "json": "^9.0.6",
    "mocha": "^5.2.0",
    "nyc": "^13.0.1",
    "rimraf": "^2.6.1",
    "sinon": "^6.3.5"
  },
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/makeomatic/mservice-social.git"
  },
  "engines": {
    "node": ">= 8.9.0",
    "npm": ">= 3.0.0"
  },
  "nyc": {
    "include": [
      "src/**/*.js"
    ],
    "exclude": [
      "src/**/*.spec.js"
    ],
    "require": [
      "@babel/register"
    ],
    "sourceMap": false,
    "instrument": false,
    "cache": true,
    "reporter": [
      "lcov",
      "json",
      "text-summary"
    ]
  }
}
